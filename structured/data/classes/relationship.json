      },
      {
        "relation_type": "scales_down_to",
        "description": "Downscaling relationship",
        "examples": ["regional-climate scales_down_to local-weather", "basin-model scales_down_to sub-basin"]
      }
    ],
    "process_relationships": [
      {
        "relation_type": "precedes",
        "description": "Sequential process relationship",
        "examples": ["precipitation precedes surface-runoff", "infiltration precedes groundwater-recharge"]
      },
      {
        "relation_type": "interacts_with",
        "description": "Process interaction",
        "examples": ["surface-water interacts_with groundwater", "evapotranspiration interacts_with soil-moisture"]
      },
      {
        "relation_type": "competes_with",
        "description": "Competing processes",
        "examples": ["evaporation competes_with transpiration", "infiltration competes_with surface-runoff"]
      }
    ],
    "spatial_relationships": [
      {
        "relation_type": "upstream_of",
        "description": "Upstream spatial relationship",
        "examples": ["headwater upstream_of outlet", "tributary upstream_of main-stem"]
      },
      {
        "relation_type": "downstream_of",
        "description": "Downstream spatial relationship",
        "examples": ["outlet downstream_of headwater", "estuary downstream_of river"]
      },
      {
        "relation_type": "adjacent_to",
        "description": "Spatial adjacency",
        "examples": ["riparian-zone adjacent_to stream", "floodplain adjacent_to river"]
      },
      {
        "relation_type": "within",
        "description": "Spatial containment",
        "examples": ["sub-basin within watershed", "stream within catchment"]
      }
    ],
    "temporal_relationships": [
      {
        "relation_type": "follows",
        "description": "Temporal sequence",
        "examples": ["wet-season follows dry-season", "snowmelt follows snow-accumulation"]
      },
      {
        "relation_type": "concurrent_with",
        "description": "Simultaneous occurrence",
        "examples": ["peak-flow concurrent_with peak-precipitation", "drought concurrent_with low-streamflow"]
      },
      {
        "relation_type": "lags_behind",
        "description": "Time lag relationship",
        "examples": ["streamflow lags_behind precipitation", "groundwater-response lags_behind recharge"]
      }
    ],
    "impact_relationships": [
      {
        "relation_type": "affects",
        "description": "General impact relationship",
        "examples": ["land-use-change affects hydrology", "climate-variability affects water-resources"]
      },
      {
        "relation_type": "threatens",
        "description": "Negative impact or threat",
        "examples": ["flooding threatens infrastructure", "drought threatens water-supply"]
      },
      {
        "relation_type": "benefits",
        "description": "Positive impact",
        "examples": ["wetlands benefits water-quality", "conservation benefits streamflow"]
      }
    ]
