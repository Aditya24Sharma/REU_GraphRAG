      },
      {
        "relation_type": "aggregates_to",
        "description": "Spatial or temporal aggregation",
        "examples": ["daily-data aggregates_to monthly-data", "point-measurements aggregates_to basin-average"]
      },
      {
        "relation_type": "correlates_with",
        "description": "Statistical correlation",
        "examples": ["precipitation correlates_with streamflow", "temperature correlates_with evaporation"]
      }
    ],
    "scale_relationships": [
      {
        "relation_type": "scales_up_to",
