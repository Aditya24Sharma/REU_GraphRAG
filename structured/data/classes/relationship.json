        "examples": ["climate-change causes increased-precipitation", "urbanization causes surface-runoff-increase"]
      },
      {
        "relation_type": "influences",
        "description": "Indirect influence or correlation",
        "examples": ["vegetation-cover influences evapotranspiration", "soil-type influences infiltration-rate"]
      },
      {
        "relation_type": "controls",
        "description": "Strong controlling factor",
        "examples": ["topography controls flow-direction", "precipitation controls streamflow-generation"]
      }
    ],
    "methodological_relationships": [
